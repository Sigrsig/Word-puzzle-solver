{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "testing-library", "jest"],
  "rules": {
    "testing-library/no-debugging-utils": 0,
    "no-unused-vars": 0
  }
}
